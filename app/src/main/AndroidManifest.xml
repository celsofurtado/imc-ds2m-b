<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.senai.sp.jandira.imc_app_b">

    <application
        android:allowBackup="true"
        android:icon="@drawable/peso"
        android:label="@string/app_name"
        android:roundIcon="@drawable/peso"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ResultadoNcdActivity" />
        <activity android:name=".NcdActivity" />
        <activity android:name=".ResultadoImcActivity" />
        <activity android:name=".ImcActivity" />
        <activity android:name=".MainActivity"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>